ied Coronaviruses.", "access_right": "restricted", "creators": [{"name": "Souris, Marc", "affiliation": null}, {"name": "Tshilolo, L\u00e9on", "affiliation": null}, {"name": "Parzy, Daniel", "affiliation": null}, {"name": "Lobaloba Ingoba, Line", "affiliation": null}, {"name": "Ntoumi, Francine", "affiliation": null}, {"name": "Kamgaing, Rachel", "affiliation": null}, {"name": "Ndour, Moussa", "affiliation": null}, {"name": "Mbongi, Destin", "affiliation": null}, {"name": "Phoba, Balthazar", "affiliation": null}, {"name": "Tshilolo, Marie-Anasthasie", "affiliation": null}, {"name": "Mbungu, Ren\u00e9", "affiliation": null}, {"name": "Sosso, Martin Samuel", "affiliation": null}, {"name": "Fainguem, Nadine", "affiliation": null}, {"name": "Ndiaye Dieye, Tandakha", "affiliation": null}, {"name": "Sylla, Massamba", "affiliation": null}, {"name": "Morand, Pierre", "affiliation": null}, {"name": "Gonzalez, Jean-Paul", "affiliation": null}], "keywords": ["Biodiversity", "Mammalia", "Chiroptera", "Chordata", "Animalia", "bats", "bat"], "related_identifiers": [{"identifier": "hash://md5/eeea92c88f0e830709c1cdd1c7a761cc", "relation": "hasVersion", "scheme": "url"}, {"identifier": "hash://sha256/acc261a81ce2bdd11ce9098ff17513f33eebe57ca56e761e2890b3ed9406a93f", "relation": "hasVersion", "scheme": "url"}, {"identifier": "zotero://select/groups/5435545/items/47EQU9EC", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://zotero.org/groups/5435545/items/47EQU9EC", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://linker.bio/cut:hash://md5/610ff242e5332ee592fdbf00091f6d3f!/b193607-196070", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "hash://md5/ce1e7618f237ff56e74d51f496e811a3", "relation": "isPartOf", "scheme": "url"}, {"identifier": "10.5281/zenodo.1410543", "relation": "isCompiledBy", "resource_type": "software", "scheme": "doi"}], "custom": {"dwc:class": ["Mammalia"], "dwc:kingdom": ["Animalia"], "dwc:order": ["Chiropter