at"], "related_identifiers": [{"identifier": "hash://md5/c796fc586934a9c32ec2dc7a4aac4cff", "relation": "hasVersion", "scheme": "url"}, {"identifier": "hash://sha256/1103e373f5e9b0224d7af6391c815b5c413023c10aea453a21b720590e7b13c0", "relation": "hasVersion", "scheme": "url"}, {"identifier": "zotero://select/groups/5435545/items/G5Q3RUWG", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://zotero.org/groups/5435545/items/G5Q3RUWG", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://linker.bio/cut:hash://md5/4c072bae5eca5062a0b86685d16a7953!/b220834-223357", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "hash://md5/330417efcaa2560c9b8ebcd6fae17ce0", "relation": "isPartOf", "scheme": "url"}, {"identifier": "10.5281/zenodo.1410543", "relation": "isCompiledBy", "resource_type": "software", "scheme": "doi"}], "custom": {"dwc:class": ["Mammalia"], "dwc:kingdom": ["Animalia"], "dwc:order": ["Chiroptera"], "dwc:phylum": ["Chordata"]}, "resource_type": {"title": "Journal article", "type": "publication", "subtype": "article"}, "journal": {"issue": "1", "pages": "18-28", "title": "Journal of Medical Entomology", "volume": "56"}, "alternate_identifiers": [{"identifier": "hash://md5/c796fc586934a9c32ec2dc7a4aac4cff"}, {"identifier": "urn:lsid:zotero.org:groups:5435545:items:G5Q3RUWG"}, {"identifier": "10.1093/jme/tjy166"}], "communities": [{"id": "batlit"}, {"id": "biosyslit"}], "relations": {"version": [{"index": 0, "is_last": true, "parent": {"pid_type": "recid", "pid_value": "13476331"}}]}}, "title": "New Geographical Records of Bat Flies (Diptera: Streblidae) Associated With Phyllostomid Bats (Chiroptera: Phyllostomidae) in the West Highlands of Mexico", "links": {"self": "https://zenodo.org/api/records/13476332", "self_html": "https://zenodo.org/records/13476332", "preview_html": "https://zenodo.org/records/13476332?preview=1", "doi": "https://doi.org/10.5281/zenodo.13476332", "self_doi": "https://doi.org/10.5281/zenodo