s.", "access_right": "restricted", "creators": [{"name": "Niga, Yoshifumi", "affiliation": null}, {"name": "Fujioka, Emyo", "affiliation": null}, {"name": "Heim, Olga", "affiliation": null}, {"name": "Nomi, Akito", "affiliation": null}, {"name": "Fukui, Dai", "affiliation": null}, {"name": "Hiryu, Shizuko", "affiliation": null}], "keywords": ["Biodiversity", "Mammalia", "Chiroptera", "Chordata", "Animalia", "bats", "bat"], "related_identifiers": [{"identifier": "hash://md5/7d3ba3e5ad4b433b32dc78b5632a319e", "relation": "hasVersion", "scheme": "url"}, {"identifier": "hash://sha256/ee805ab4429dcd645fc84923d69b471f01425b91838149d0e14e27abf0af82bd", "relation": "hasVersion", "scheme": "url"}, {"identifier": "zotero://select/groups/5435545/items/EIBL234H", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://zotero.org/groups/5435545/items/EIBL234H", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://linker.bio/cut:hash://md5/c2413f1ccb7c86de0a34921279722e22!/b197793-200251", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "hash://md5/330417efcaa2560c9b8ebcd6fae17ce0", "relation": "isPartOf", "scheme": "url"}, {"identifier": "10.5281/zenodo.1410543", "relation": "isCompiledBy", "resource_type": "software", "scheme": "doi"}], "custom": {"dwc:class": ["Mammalia"], "dwc:kingdom": ["Animalia"], "dwc:order": ["Chiroptera"], "dwc:phylum": ["Chordata"]}, "resource_type": {"title": "Journal article", "type": "publication", "subtype": "article"}, "journal": {"issue": "6", "pages": "230035", "title": "Royal Society Open Science", "volume": "10"}, "alternate_identifiers": [{"identifier": "hash://md5/7d3ba3e5ad4b433b32dc78b5632a319e"}, {"identifier": "urn:lsid:zotero.org:groups:5435545:items:EIBL234H"}, {"identifier": "10.1098/rsos.230035"}], "communities": [{"id": "batlit"}, {"id": "biosyslit"}], "relations": {"version": [{"index": 0, "is_last": true, "parent": {"pid_type": "recid", "pid_value": "13439957"}}]}}, "title": "A glimps