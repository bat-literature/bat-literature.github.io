s.", "access_right": "restricted", "creators": [{"name": "Mullin, Katherine E.", "affiliation": null}, {"name": "Yoh, Natalie", "affiliation": null}, {"name": "Mitchell, Simon L.", "affiliation": null}, {"name": "Basrur, Saloni", "affiliation": null}, {"name": "Seaman, Dave J. I.", "affiliation": null}, {"name": "Bernard, Henry", "affiliation": null}, {"name": "Struebig, Matthew J.", "affiliation": null}], "keywords": ["Biodiversity", "Mammalia", "Chiroptera", "Chordata", "Animalia", "bats", "bat"], "related_identifiers": [{"identifier": "hash://md5/b6033d759df10448130d5b8e06425ec3", "relation": "hasVersion", "scheme": "url"}, {"identifier": "hash://sha256/2b83cf892b67e6aa2eefd6b8fcc9a20c534a4675b504fae6f29e63e1b478dd15", "relation": "hasVersion", "scheme": "url"}, {"identifier": "zotero://select/groups/5435545/items/ZXGFNIY3", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://zotero.org/groups/5435545/items/ZXGFNIY3", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://linker.bio/cut:hash://md5/317dd32163e550f8b6f31f0972940706!/b29634-32098", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "hash://md5/330417efcaa2560c9b8ebcd6fae17ce0", "relation": "isPartOf", "scheme": "url"}, {"identifier": "10.5281/zenodo.1410543", "relation": "isCompiledBy", "resource_type": "software", "scheme": "doi"}], "custom": {"dwc:class": ["Mammalia"], "dwc:kingdom": ["Animalia"], "dwc:order": ["Chiroptera"], "dwc:phylum": ["Chordata"]}, "resource_type": {"title": "Journal article", "type": "publication", "subtype": "article"}, "journal": {"pages": "73", "title": "Frontiers in Forests and Global Change", "volume": "3"}, "alternate_identifiers": [{"identifier": "hash://md5/b6033d759df10448130d5b8e06425ec3"}, {"identifier": "urn:lsid:zotero.org:groups:5435545:items:ZXGFNIY3"}, {"identifier": "10.3389/ffgc.2020.00073"}], "communities": [{"id": "biosyslit"}, {"id": "batlit"}], "relations": {"version": [{"index": 0, "is_last": true, "parent