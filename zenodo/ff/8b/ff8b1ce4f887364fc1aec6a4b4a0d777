"], "related_identifiers": [{"identifier": "hash://md5/8325d8b2947b046afa8a046ce8d69d32", "relation": "hasVersion", "scheme": "url"}, {"identifier": "hash://sha256/6eae907bcc55099278da29bca2f13e7bebd4b257f25dcaee8b3db36722f1c02b", "relation": "hasVersion", "scheme": "url"}, {"identifier": "zotero://select/groups/5435545/items/295GFPXS", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://zotero.org/groups/5435545/items/295GFPXS", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://linker.bio/cut:hash://md5/1f6fdcf5b6f060922540ed280a3167c1!/b68607-71107", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "hash://md5/ce1e7618f237ff56e74d51f496e811a3", "relation": "isPartOf", "scheme": "url"}, {"identifier": "10.5281/zenodo.1410543", "relation": "isCompiledBy", "resource_type": "software", "scheme": "doi"}], "custom": {"dwc:class": ["Mammalia"], "dwc:kingdom": ["Animalia"], "dwc:order": ["Chiroptera"], "dwc:phylum": ["Chordata"]}, "resource_type": {"title": "Journal article", "type": "publication", "subtype": "article"}, "journal": {"issue": "1", "pages": "001-020", "title": "Mastozoolog\u00eda Neotropical", "volume": "28"}, "alternate_identifiers": [{"identifier": "hash://md5/8325d8b2947b046afa8a046ce8d69d32"}, {"identifier": "urn:lsid:zotero.org:groups:5435545:items:295GFPXS"}, {"identifier": "10.31687/saremMN.21.28.1.0.12"}], "communities": [{"id": "biosyslit"}, {"id": "batlit"}], "relations": {"version": [{"index": 0, "is_last": true, "parent": {"pid_type": "recid", "pid_value": "13431287"}}]}}, "title": "Patrones biogeogr\u00e1ficos de los murci\u00e9lagos de Argentina: riqueza de especies y congruencia distribucional", "links": {"self": "https://zenodo.org/api/records/13431288", "self_html": "https://zenodo.org/records/13431288", "doi": "https://doi.org/10.5281/zenodo.13431288", "self_doi": "https://doi.org/10.5281/zenodo.13431288", "self_doi_html": "https://zenodo.org/doi/10.5281/zenodo.13431288", "parent": "http