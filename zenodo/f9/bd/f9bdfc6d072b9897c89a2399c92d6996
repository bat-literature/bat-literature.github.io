s.", "access_right": "restricted", "creators": [{"name": "Kuhn, Jens H.", "affiliation": null}, {"name": "Andersen, Kristian G.", "affiliation": null}, {"name": "B\u00e0o, Y\u012bm\u00edng", "affiliation": null}, {"name": "Bavari, Sina", "affiliation": null}, {"name": "Becker, Stephan", "affiliation": null}, {"name": "Bennett, Richard S.", "affiliation": null}, {"name": "Bergman, Nicholas H.", "affiliation": null}, {"name": "Blinkova, Olga", "affiliation": null}, {"name": "Bradfute, Steven", "affiliation": null}, {"name": "Brister, J. Rodney", "affiliation": null}, {"name": "Bukreyev, Alexander", "affiliation": null}, {"name": "Chandran, Kartik", "affiliation": null}, {"name": "Chepurnov, Alexander A.", "affiliation": null}, {"name": "Davey, Robert A.", "affiliation": null}, {"name": "Dietzgen, Ralf G.", "affiliation": null}, {"name": "Doggett, Norman A.", "affiliation": null}, {"name": "Dolnik, Olga", "affiliation": null}, {"name": "Dye, John M.", "affiliation": null}, {"name": "Enterlein, Sven", "affiliation": null}, {"name": "Fenimore, Paul W.", "affiliation": null}, {"name": "Formenty, Pierre", "affiliation": null}, {"name": "Freiberg, Alexander N.", "affiliation": null}, {"name": "Garry, Robert F.", "affiliation": null}, {"name": "Garza, Nicole L.", "affiliation": null}, {"name": "Gire, Stephen K.", "affiliation": null}, {"name": "Gonzalez, Jean-Paul", "affiliation": null}, {"name": "Griffiths, Anthony", "affiliation": null}, {"name": "Happi, Christian T.", "affiliation": null}, {"name": "Hensley, Lisa E.", "affiliation": null}, {"name": "Herbert, Andrew S.", "affiliation": null}, {"name": "Hevey, Michael C.", "affiliation": null}, {"name": "Hoenen, Thomas", "affiliation": null}, {"name": "Honko, Anna N.", "affiliation": null}, {"name": "Ignatyev, Georgy M.", "affiliation": null}, {"name": "Jahrling, Peter B.", "affiliation": null}, {"name": "Johnson, Joshua C.", "affiliation": null}, {"name": "Johnson, Karl M.", "affiliation": null}, {"name": "Kindrachuk, Jaso