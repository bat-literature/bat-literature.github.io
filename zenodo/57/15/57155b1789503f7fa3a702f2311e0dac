 populations.", "access_right": "restricted", "creators": [{"name": "Kelm, Detlev H.", "affiliation": null}, {"name": "Langheld, Manuel", "affiliation": null}, {"name": "Nogueras, Jes\u00fas", "affiliation": null}, {"name": "Popa-Lisseanu, Ana G.", "affiliation": null}, {"name": "Ib\u00e1\u00f1ez, Carlos", "affiliation": null}], "keywords": ["Biodiversity", "Mammalia", "Chiroptera", "Chordata", "Animalia", "bats", "bat"], "related_identifiers": [{"identifier": "hash://md5/d5d806dfe414124bfcaa98f76d24c756", "relation": "hasVersion", "scheme": "url"}, {"identifier": "hash://sha256/d2c85f3ad037c97c8858176aa3134d9e2139c1d1984e5d363eae565868f18dff", "relation": "hasVersion", "scheme": "url"}, {"identifier": "zotero://select/groups/5435545/items/SZ85CXNK", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://zotero.org/groups/5435545/items/SZ85CXNK", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "https://linker.bio/cut:hash://md5/fa1dc749acc5c074acdb90c9347f96cf!/b245193-247642", "relation": "isDerivedFrom", "scheme": "url"}, {"identifier": "hash://md5/330417efcaa2560c9b8ebcd6fae17ce0", "relation": "isPartOf", "scheme": "url"}, {"identifier": "10.5281/zenodo.1410543", "relation": "isCompiledBy", "resource_type": "software", "scheme": "doi"}], "custom": {"dwc:class": ["Mammalia"], "dwc:kingdom": ["Animalia"], "dwc:order": ["Chiroptera"], "dwc:phylum": ["Chordata"]}, "resource_type": {"title": "Journal article", "type": "publication", "subtype": "article"}, "journal": {"issue": "8", "pages": "230309", "title": "Royal Society Open Science", "volume": "10"}, "alternate_identifiers": [{"identifier": "hash://md5/d5d806dfe414124bfcaa98f76d24c756"}, {"identifier": "urn:lsid:zotero.org:groups:5435545:items:SZ85CXNK"}, {"identifier": "10.1098/rsos.230309"}], "communities": [{"id": "batlit"}, {"id": "biosyslit"}], "relations": {"version": [{"index": 0, "is_last": true, "parent": {"pid_type": "recid", "pid_value": "13446934"}}]}}, "title": "Continuou