        "data": {
            "key": "Y8HQLN4D",
            "version": 17724,
            "itemType": "journalArticle",
            "title": "Impacts of seasonality on gene expression in the Chinese horseshoe bat",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Wenli",
                    "lastName": "Chen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Xiuguang",
                    "lastName": "Mao"
                }
