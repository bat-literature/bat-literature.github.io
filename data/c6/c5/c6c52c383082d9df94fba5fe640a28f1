            "itemType": "journalArticle",
            "title": "Spatial patterns of bat diversity overlap with woodpecker abundance",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Dorota",
                    "lastName": "Kotowska"
                },
                {
                    "creatorType": "author",
                    "firstName": "Marcin",
                    "lastName": "Zegarek"
                },
                {
                    "creatorType": "author",
                    "firstName": "Grzegorz",
                    "lastName": "Osojca"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrzej",
                    "lastName": "Satory"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tomas",
                    "lastName": "Pärt"
