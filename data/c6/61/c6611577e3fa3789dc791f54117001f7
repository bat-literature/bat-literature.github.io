            "version": 53820,
            "itemType": "journalArticle",
            "title": "Is species identity, sex, age or individual quality conveyed by echolocation call frequency in European horseshoe bats?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Björn M.",
                    "lastName": "Siemers"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kristian",
                    "lastName": "Beedholm"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christian",
                    "lastName": "Dietz"
                },
                {
                    "creatorType": "author",
                    "firstName": "Isabel",
                    "lastName": "Dietz"
