            "itemType": "journalArticle",
            "title": "Evolutionary insights into Rhinolophus episcopus (Chiroptera, Rhinolophidae) in China: Isolation by distance, environment, or sensory system?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Tong",
                    "lastName": "Liu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kangkang",
                    "lastName": "Zhang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wentao",
                    "lastName": "Dai"
                },
                {
                    "creatorType": "author",
                    "firstName": "Longru",
                    "lastName": "Jin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Keping",
                    "lastName": "Sun"
