            "itemType": "journalArticle",
            "title": "Using very-high-resolution satellite imagery and deep learning to detect and count African elephants in heterogeneous landscapes",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Isla",
                    "lastName": "Duporge"
                },
                {
                    "creatorType": "author",
                    "firstName": "Olga",
                    "lastName": "Isupova"
                },
                {
                    "creatorType": "author",
                    "firstName": "Steven",
                    "lastName": "Reece"
                },
