            "itemType": "journalArticle",
            "title": "White-nose syndrome restructures bat skin microbiomes",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Meghan",
                    "lastName": "Ange-Stark"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tina L",
                    "lastName": "Cheng"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joseph R",
                    "lastName": "Hoyt"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kate E",
                    "lastName": "Langwig"
                },
                {
                    "creatorType": "author",
                    "firstName": "Winifred F",
                    "lastName": "Frick"
                },
                {
                    "creatorType": "author",
                    "firstName": "A Marm",
                    "lastName": "Kilpatrick"
