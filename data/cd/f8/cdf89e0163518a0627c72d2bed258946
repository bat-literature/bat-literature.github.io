            "itemType": "journalArticle",
            "title": "Operator bias in software‐aided bat call identification",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Georg",
                    "lastName": "Fritsch"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexander",
                    "lastName": "Bruckner"
                }
