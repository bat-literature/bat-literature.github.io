            "key": "NDNVCKEA",
            "version": 56452,
            "itemType": "journalArticle",
            "title": "Managing hedgerows for nocturnal wildlife: Do bats and their insect prey benefit from targeted agri‐environment schemes?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jérémy S. P.",
                    "lastName": "Froidevaux"
                },
                {
                    "creatorType": "author",
                    "firstName": "Katherine L.",
                    "lastName": "Boughey"
                },
                {
                    "creatorType": "author",
                    "firstName": "Charlotte L.",
                    "lastName": "Hawkins"
                },
                {
                    "creatorType": "author",
                    "firstName": "Moth",
                    "lastName": "Broyles"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gareth",
                    "lastName": "Jones"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Matthew",
                    "lastName": "Struebig"
                }
