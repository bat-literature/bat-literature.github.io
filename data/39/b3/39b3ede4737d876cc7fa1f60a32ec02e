            "key": "ZEXCAIX2",
            "version": 28924,
            "itemType": "journalArticle",
            "title": "Insectivorous bats foraging in cotton crop interiors is driven by moon illumination and insect abundance, but diversity benefits from woody vegetation cover",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Heidi",
                    "lastName": "Kolkert"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rhiannon",
                    "lastName": "Smith"
                },
                {
                    "creatorType": "author",
                    "firstName": "Romina",
                    "lastName": "Rader"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nick",
                    "lastName": "Reid"
                }
