            "itemType": "journalArticle",
            "title": "Histological assessment of cellular immune response to the phytohemagglutinin skin test in Brazilian free-tailed bats (Tadarida brasiliensis)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Amy S.",
                    "lastName": "Turmelle"
                },
                {
                    "creatorType": "author",
                    "firstName": "James A.",
                    "lastName": "Ellison"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mary T.",
