        "data": {
            "key": "RJE55PZL",
            "version": 57602,
            "itemType": "journalArticle",
            "title": "Bats in the Ghats: Agricultural intensification reduces functional diversity and increases trait filtering in a biodiversity hotspot in India",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Claire F.R.",
                    "lastName": "Wordley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mahesh",
                    "lastName": "Sankaran"
