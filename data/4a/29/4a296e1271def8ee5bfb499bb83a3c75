            "version": 58849,
            "itemType": "journalArticle",
            "title": "ECTOPARASITES OF THE OCCULT BAT, MYOTIS OCCULTUS (CHIROPTERA: VESPERTILIONIDAE)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ernest W.",
                    "lastName": "Valdez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christopher M.",
                    "lastName": "Ritzi"
