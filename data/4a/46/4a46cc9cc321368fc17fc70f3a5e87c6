            "itemType": "journalArticle",
            "title": "Multifaceted DNA metabarcoding of guano to uncover multiple classes of ecological data in two different bat communities",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Richard F.",
                    "lastName": "Lance"
                },
                {
                    "creatorType": "author",
                    "firstName": "Xin",
                    "lastName": "Guan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joel F.",
                    "lastName": "Swift"
                },
