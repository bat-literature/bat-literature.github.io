            "version": 35320,
            "itemType": "journalArticle",
            "title": "Effectiveness of Acoustic Lures for Increasing Indiana Bat Captures in Mist-Nets",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Stephen T.",
                    "lastName": "Samoray"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mark W.",
                    "lastName": "Gumbert"
                },
                {
                    "creatorType": "author",
                    "firstName": "Piper L.",
                    "lastName": "Roby"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gregg A.",
                    "lastName": "Janos"
                },
