            "version": 28902,
            "itemType": "journalArticle",
            "title": "Activity of Insectivorous Bats Associated with Cattle Ponds at La Michilía Biosphere Reserve, Durango, Mexico: Implications for Conservation",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Celia",
                    "lastName": "López-González"
                },
                {
                    "creatorType": "author",
                    "firstName": "Emma P.",
                    "lastName": "Gómez-Ruiz"
                },
