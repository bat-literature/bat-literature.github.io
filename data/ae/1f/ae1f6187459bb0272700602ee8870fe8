            "key": "VL75ES5X",
            "version": 29035,
            "itemType": "journalArticle",
            "title": "Bat Pass Duration Measurement: An Indirect Measure of Distance of Detection",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Christian",
                    "lastName": "Kerbiriou"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yves",
                    "lastName": "Bas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Isabelle",
                    "lastName": "Le Viol"
