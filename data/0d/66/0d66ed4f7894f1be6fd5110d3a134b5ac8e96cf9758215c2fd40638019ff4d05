            "version": 226,
            "itemType": "journalArticle",
            "title": "Evolutionary inferences of novel simian T lymphotropic virus type 1 from wild-caught chacma (Papio ursinus) and olive baboons (Papio anubis)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "R.",
                    "lastName": "Mahieux"
                },
                {
                    "creatorType": "author",
                    "firstName": "J.",
                    "lastName": "Pecon-Slattery"
                },
                {
                    "creatorType": "author",
