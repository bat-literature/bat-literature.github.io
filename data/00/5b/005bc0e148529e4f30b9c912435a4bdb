            "version": 56220,
            "itemType": "journalArticle",
            "title": "Pronounced seasonal changes in the movement ecology of a highly gregarious central-place forager, the African straw-coloured fruit bat (Eidolon helvum)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "J.",
                    "lastName": "Fahr"
                },
                {
                    "creatorType": "author",
                    "firstName": "M.",
                    "lastName": "Abedi-Lartey"
                },
                {
                    "creatorType": "author",
                    "firstName": "T.",
                    "lastName": "Esch"
                },
                {
                    "creatorType": "author",
                    "firstName": "M.",
