            "key": "6ZT9U43D",
            "version": 56207,
            "itemType": "journalArticle",
            "title": "Studying the microbiota of bats: Accuracy of direct and indirect samplings",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Muriel",
                    "lastName": "Dietrich"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wanda",
                    "lastName": "Markotter"
                }
