            "itemType": "journalArticle",
            "title": "Pollination partners of Mucuna macrocarpa (Fabaceae) at the northern limit of its range",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Shun",
                    "lastName": "Kobayashi"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tetsuo",
                    "lastName": "Denda"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shigehiko",
                    "lastName": "Mashiba"
                },
                {
                    "creatorType": "author",
                    "firstName": "Toshitaka",
                    "lastName": "Iwamoto"
                },
                {
                    "creatorType": "author",
                    "firstName": "Teruo",
                    "lastName": "Doi"
