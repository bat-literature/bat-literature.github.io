            "version": 54561,
            "itemType": "journalArticle",
            "title": "Bromeliads going batty: pollinator partitioning among sympatric chiropterophilous Bromeliaceae",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Pedro Adrián",
                    "lastName": "Aguilar-Rodríguez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Marco",
                    "lastName": "Tschapka"
                },
                {
                    "creatorType": "author",
                    "firstName": "José G",
                    "lastName": "García-Franco"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thorsten",
                    "lastName": "Krömer"
                },
