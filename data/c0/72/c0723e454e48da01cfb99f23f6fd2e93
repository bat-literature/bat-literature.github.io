                {
                    "creatorType": "author",
                    "firstName": "Shimon",
                    "lastName": "Harrus"
                },
                {
                    "creatorType": "author",
                    "firstName": "Reuben A.",
                    "lastName": "Ocholi"
                },
                {
                    "creatorType": "author",
                    "firstName": "Irene I.",
