            "version": 58202,
            "itemType": "journalArticle",
            "title": "Habitat disturbance results in chronic stress and impaired health status in forest-dwelling paleotropical bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Anne",
                    "lastName": "Seltmann"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gábor Á.",
                    "lastName": "Czirják"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexandre",
