            "key": "NZN4GXMW",
            "version": 54888,
            "itemType": "journalArticle",
            "title": "Sergey Ivanovitch Ognev and the formation of theriology in Russia",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Irina",
                    "lastName": "Bakloushinskaya"
                },
                {
                    "creatorType": "author",
                    "firstName": "Georgii A.",
                    "lastName": "Bazykin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexey P.",
