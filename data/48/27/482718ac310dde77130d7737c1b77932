            "version": 54918,
            "itemType": "journalArticle",
            "title": "Diversity patterns of small mammals in the Zambales Mts., Luzon, Philippines",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Danilo S.",
                    "lastName": "Balete"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lawrence R.",
                    "lastName": "Heaney"
                },
                {
                    "creatorType": "author",
                    "firstName": "Maria",
