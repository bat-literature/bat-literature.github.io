            "version": 41564,
            "itemType": "journalArticle",
            "title": "Systematics and phylogeography of the steppe whiskered bat Myotis aurascens Kuzyakin, 1935 (Chiroptera, Vespertilionidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "K.",
                    "lastName": "Tsytsulina"
                },
                {
                    "creatorType": "author",
                    "firstName": "M.H.",
                    "lastName": "Dick"
                },
                {
                    "creatorType": "author",
                    "firstName": "K.",
