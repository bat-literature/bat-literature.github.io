            "itemType": "journalArticle",
            "title": "Molecular phylogeny of New World Myotis (Chiroptera, Vespertilionidae) inferred from mitochondrial and nuclear DNA genes",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "B.",
                    "lastName": "Stadelmann"
                },
                {
                    "creatorType": "author",
                    "firstName": "L.-K.",
                    "lastName": "Lin"
                },
