            "version": 61327,
            "itemType": "journalArticle",
            "title": "Bats are sentinels for invasive pest surveillance based on DNA metabarcoding",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Yingying",
                    "lastName": "Liu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Man",
                    "lastName": "Si"
                },
                {
                    "creatorType": "author",
                    "firstName": "Zhenglanyi",
