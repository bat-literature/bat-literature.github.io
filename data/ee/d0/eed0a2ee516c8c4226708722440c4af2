            "key": "8JYXGZ8E",
            "version": 57466,
            "itemType": "journalArticle",
            "title": "Road density and forest fragmentation shape bat communities in temperate mosaic landscapes",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Alexis",
                    "lastName": "Laforge"
                },
                {
                    "creatorType": "author",
                    "firstName": "Luc",
                    "lastName": "Barbaro"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yves",
                    "lastName": "Bas"
                },
                {
                    "creatorType": "author",
                    "firstName": "François",
                    "lastName": "Calatayud"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sylvie",
                    "lastName": "Ladet"
                },
                {
                    "creatorType": "author",
                    "firstName": "Clélia",
