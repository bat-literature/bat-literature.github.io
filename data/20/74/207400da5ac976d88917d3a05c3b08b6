            "key": "UE2GVPAV",
            "version": 55688,
            "itemType": "journalArticle",
            "title": "Design and Psychometric Properties of the BAtSS: A New Tool to Assess Attitudes towards Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Beatriz",
                    "lastName": "Pérez"
                },
                {
                    "creatorType": "author",
                    "firstName": "Boris",
                    "lastName": "Álvarez"
                },
