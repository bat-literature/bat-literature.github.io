            "key": "MWE3MEMH",
            "version": 134,
            "itemType": "bookSection",
            "title": "Convergence in Gliding Animals: Morphology, Behavior, and Mechanics",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Pranav C.",
                    "lastName": "Khandelwal"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shane D.",
                    "lastName": "Ross"
                },
                {
                    "creatorType": "author",
                    "firstName": "Haibo",
                    "lastName": "Dong"
                },
                {
                    "creatorType": "author",
                    "firstName": "John J.",
                    "lastName": "Socha"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Vincent L.",
                    "lastName": "Bels"
                },
