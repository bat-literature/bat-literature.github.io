            "version": 145,
            "itemType": "journalArticle",
            "title": "CTLA4-Ig restores rejection of MHC class-II mismatched allografts by disabling IL-2-expanded regulatory T cells",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "L. M.",
                    "lastName": "Charbonnier"
                },
                {
                    "creatorType": "author",
                    "firstName": "B.",
                    "lastName": "Vokaer"
                },
                {
                    "creatorType": "author",
                    "firstName": "P. H.",
                    "lastName": "Lematre"
                },
                {
                    "creatorType": "author",
                    "firstName": "K. A.",
                    "lastName": "Field"
                },
                {
                    "creatorType": "author",
                    "firstName": "O.",
