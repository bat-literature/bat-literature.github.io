                },
                {
                    "creatorType": "author",
                    "firstName": "Joacil Germano",
                    "lastName": "Soares"
                },
                {
                    "creatorType": "author",
                    "firstName": "Fernando V. L.",
                    "lastName": "Ladd"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jeferson S.",
                    "lastName": "Cavalcante"
                },
                {
                    "creatorType": "author",
                    "firstName": "Judney C.",
                    "lastName": "Cavalcante"
                },
                {
                    "creatorType": "author",
                    "firstName": "Miriam S. M. O.",
