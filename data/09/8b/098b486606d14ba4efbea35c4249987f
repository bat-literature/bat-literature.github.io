            "version": 58528,
            "itemType": "journalArticle",
            "title": "Maternity colony social structure of myotis in British Columbia, Canada",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Leah J.",
                    "lastName": "Rensel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Karen E.",
