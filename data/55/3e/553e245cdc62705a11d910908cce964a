            "version": 57984,
            "itemType": "journalArticle",
            "title": "Phylogeny and foraging behaviour shape modular morphological variation in bat humeri",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Camilo",
                    "lastName": "López‐Aguirre"
                },
                {
                    "creatorType": "author",
                    "firstName": "Suzanne J.",
                    "lastName": "Hand"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daisuke",
                    "lastName": "Koyabu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Vuong Tan",
                    "lastName": "Tu"
