            "key": "DPB2DQXQ",
            "version": 17507,
            "itemType": "journalArticle",
            "title": "Echolocation Call Structure and Intensity in Five Species of Insectivorous Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Dean A.",
                    "lastName": "Waters"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gareth",
                    "lastName": "Jones"
                }
