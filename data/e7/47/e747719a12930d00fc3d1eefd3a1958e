        "data": {
            "key": "VEBDDLQF",
            "version": 55697,
            "itemType": "journalArticle",
            "title": "Vertical stratification of bat assemblages in flooded and unflooded Amazonian forests",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Maria João Ramos",
                    "lastName": "Pereira"
                },
                {
                    "creatorType": "author",
                    "firstName": "João Tiago",
                    "lastName": "Marques"
                },
