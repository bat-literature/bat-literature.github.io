            "itemType": "journalArticle",
            "title": "Reproduction and nutritional stress are risk factors for Hendra virus infection in little red flying foxes (Pteropus scapulatus)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Raina K",
                    "lastName": "Plowright"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hume E",
                    "lastName": "Field"
                },
                {
                    "creatorType": "author",
                    "firstName": "Craig",
                    "lastName": "Smith"
