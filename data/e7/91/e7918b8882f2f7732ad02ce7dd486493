        "data": {
            "key": "52RIZZZL",
            "version": 16928,
            "itemType": "journalArticle",
            "title": "Temporal niche overlap of a riparian forest bat assemble in subtropical Mexico",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Julio César",
                    "lastName": "Arriaga-Flores"
                },
                {
                    "creatorType": "author",
                    "firstName": "Iván",
