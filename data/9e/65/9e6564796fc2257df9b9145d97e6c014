                },
                {
                    "creatorType": "author",
                    "firstName": "Anskje",
                    "lastName": "Van Mensel"
                },
                {
                    "creatorType": "author",
                    "firstName": "François",
                    "lastName": "Chiron"
                },
                {
                    "creatorType": "author",
                    "firstName": "Florian",
