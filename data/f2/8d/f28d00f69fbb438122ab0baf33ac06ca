            "version": 56190,
            "itemType": "journalArticle",
            "title": "Species misidentification in local markets: Discrepancies between reporting and molecular identification of bushmeat species in northern Uganda",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "BreeAnna",
                    "lastName": "Dell"
                },
                {
                    "creatorType": "author",
                    "firstName": "Charles",
                    "lastName": "Masembe"
                },
                {
                    "creatorType": "author",
                    "firstName": "Richard",
                    "lastName": "Gerhold"
                },
                {
                    "creatorType": "author",
                    "firstName": "Adam",
                    "lastName": "Willcox"
                },
