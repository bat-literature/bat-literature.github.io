            "volume": "7",
            "issue": "",
            "pages": "e6844",
            "date": "2019-04-30",
            "series": "",
            "seriesTitle": "",
            "seriesText": "",
            "journalAbbreviation": "",
            "language": "",
            "DOI": "10.7717/peerj.6844",
