        "data": {
            "key": "R4FD8WIJ",
            "version": 46879,
            "itemType": "journalArticle",
            "title": "Mountain hares Lepus timidus and tourism: stress events and reactions",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Maik",
                    "lastName": "Rehnus"
                },
                {
                    "creatorType": "author",
                    "firstName": "Martin",
                    "lastName": "Wehrle"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rupert",
                    "lastName": "Palme"
