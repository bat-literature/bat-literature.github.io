            "key": "GYL3LSQU",
            "version": 432,
            "itemType": "journalArticle",
            "title": "Performance of bat-derived macrophages at different temperatures",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Monika",
                    "lastName": "Nemcova"
                },
                {
                    "creatorType": "author",
                    "firstName": "Veronika",
                    "lastName": "Seidlova"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jan",
                    "lastName": "Zukal"
                },
                {
                    "creatorType": "author",
                    "firstName": "Heliana",
