            "key": "GDCNXMCJ",
            "version": 18222,
            "itemType": "journalArticle",
            "title": "Have artificial lighting and noise pollution caused zoonosis and the COVID-19 pandemic? A review",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Shanshan",
                    "lastName": "He"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wenyuan",
                    "lastName": "Shao"
                },
