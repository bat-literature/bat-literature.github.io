            "version": 37683,
            "itemType": "journalArticle",
            "title": "Bat Boxes — A Review of Their Use and Application, Past, Present and Future",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Niels",
                    "lastName": "Rueegger"
                }
