                },
                {
                    "creatorType": "author",
                    "firstName": "Phyllis Catharina",
                    "lastName": "Romijn"
                },
                {
                    "creatorType": "author",
                    "firstName": "Danielle Bastos",
                    "lastName": "Araujo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Silvana Regina",
                    "lastName": "Favoretto"
