            "version": 35237,
            "itemType": "journalArticle",
            "title": "Evaluation of Total Body Electrical Conductivity to Estimate Body Composition of a Small Mammal",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "D. Scott",
                    "lastName": "Reynolds"
                },
                {
                    "creatorType": "author",
                    "firstName": "James C.",
                    "lastName": "Sullivan"
                },
