            "version": 35220,
            "itemType": "journalArticle",
            "title": "Which factors influence the occurrence and density of tree microhabitats in Mediterranean oak forests?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Baptiste",
                    "lastName": "Regnery"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yoan",
                    "lastName": "Paillet"
                },
                {
                    "creatorType": "author",
                    "firstName": "Denis",
