            "key": "FULEQFLQ",
            "version": 17894,
            "itemType": "journalArticle",
            "title": "Aposematic Sounds in African Moths",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Dorothy C.",
                    "lastName": "Dunning"
                },
                {
                    "creatorType": "author",
                    "firstName": "Martin",
                    "lastName": "Kruger"
