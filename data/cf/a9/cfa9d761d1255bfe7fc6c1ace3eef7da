                {
                    "creatorType": "author",
                    "firstName": "Sylvie Djègo",
                    "lastName": "Djossou"
                },
                {
                    "creatorType": "author",
                    "firstName": "Guy Apollinaire",
                    "lastName": "Mensah"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jiliang",
