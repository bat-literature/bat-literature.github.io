            "version": 56773,
            "itemType": "journalArticle",
            "title": "SPECIES‐SPECIFIC PATTERNS OF BAT ACTIVITY IN AN URBAN LANDSCAPE",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Stanley D.",
                    "lastName": "Gehrt"
                },
                {
                    "creatorType": "author",
                    "firstName": "James E.",
                    "lastName": "Chelsvig"
