            "key": "TVGQNCK2",
            "version": 38536,
            "itemType": "journalArticle",
            "title": "Fast-moving bat ears create informative Doppler shifts",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Xiaoyan",
                    "lastName": "Yin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rolf",
                    "lastName": "Müller"
