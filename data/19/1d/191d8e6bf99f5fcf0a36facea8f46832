            "version": 48698,
            "itemType": "journalArticle",
            "title": "Fitting Linear Mixed-Effects Models Using lme4",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Douglas",
                    "lastName": "Bates"
                },
                {
                    "creatorType": "author",
                    "firstName": "Martin",
