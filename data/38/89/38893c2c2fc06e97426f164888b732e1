        "data": {
            "key": "HNLRFKWM",
            "version": 58489,
            "itemType": "journalArticle",
            "title": "Phylogenetic fields of species: cross-species patterns of phylogenetic structure and geographical coexistence",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Fabricio",
                    "lastName": "Villalobos"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thiago F.",
                    "lastName": "Rangel"
