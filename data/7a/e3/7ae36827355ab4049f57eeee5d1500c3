            "key": "EGRQ8APU",
            "version": 17102,
            "itemType": "journalArticle",
            "title": "Dietary signals in dental microwear of predatory small mammals appear unaffected by extremes in environmental abrasive load",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Neil F.",
                    "lastName": "Adams"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thomas",
                    "lastName": "Gray"
                },
