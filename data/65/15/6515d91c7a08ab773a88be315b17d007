            "key": "CP6DNKZ9",
            "version": 57095,
            "itemType": "journalArticle",
            "title": "Bats of the Wildcat Hills and Surrounding Areas in Western Nebraska",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Keith",
                    "lastName": "Geluso"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jeffrey J.",
                    "lastName": "Huebschman"
