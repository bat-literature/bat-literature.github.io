        "data": {
            "key": "DGIJ62LX",
            "version": 60673,
            "itemType": "bookSection",
            "title": "Insect Migration",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jason W.",
                    "lastName": "Chapman"
                },
                {
                    "creatorType": "author",
                    "firstName": "V. Alistair",
                    "lastName": "Drake"
                }
