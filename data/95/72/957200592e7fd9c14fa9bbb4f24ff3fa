            "version": 54430,
            "itemType": "journalArticle",
            "title": "Seasonal and Geographic Trends in Acoustic Detection of Tree-Roosting Bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Joseph S.",
                    "lastName": "Johnson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kristen S.",
                    "lastName": "Watrous"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gino J.",
                    "lastName": "Giumarro"
                },
                {
                    "creatorType": "author",
                    "firstName": "Trevor S.",
                    "lastName": "Peterson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sarah A.",
