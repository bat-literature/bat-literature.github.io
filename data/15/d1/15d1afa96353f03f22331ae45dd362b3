            "itemType": "journalArticle",
            "title": "DEBrowser: interactive differential expression analysis and visualization tool for count data",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Alper",
                    "lastName": "Kucukural"
                },
                {
                    "creatorType": "author",
                    "firstName": "Onur",
                    "lastName": "Yukselen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Deniz M.",
                    "lastName": "Ozata"
                },
                {
                    "creatorType": "author",
                    "firstName": "Melissa J.",
