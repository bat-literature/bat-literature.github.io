            "key": "UK2MVYCQ",
            "version": 32520,
            "itemType": "journalArticle",
            "title": "Predictions of Mammal Diversity on Four Land Masses",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Rodrigo A.",
                    "lastName": "Medellín"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jorge",
                    "lastName": "Soberón"
                }
