                {
                    "creatorType": "author",
                    "firstName": "Patrick Jules",
                    "lastName": "Atagana"
                },
                {
                    "creatorType": "author",
                    "firstName": "Phallin Roméal",
                    "lastName": "Sandjo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hermann",
                    "lastName": "Taedoumg"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andreanna J.",
                    "lastName": "Welch"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hugo",
