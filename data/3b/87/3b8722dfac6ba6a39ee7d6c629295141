                {
                    "creatorType": "author",
                    "firstName": "Richard A.",
                    "lastName": "Bowen"
                },
                {
                    "creatorType": "author",
                    "firstName": "David L.",
                    "lastName": "Smith"
                },
                {
                    "creatorType": "author",
                    "firstName": "Thomas R.",
                    "lastName": "Stanley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Laura E.",
                    "lastName": "Ellison"
                },
