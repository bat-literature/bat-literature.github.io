            "key": "SHFEKFUT",
            "version": 57269,
            "itemType": "journalArticle",
            "title": "Bat diversity in oligotrophic forests of southern Borneo",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Matthew J.",
                    "lastName": "Struebig"
                },
                {
                    "creatorType": "author",
                    "firstName": "Birute M.F.",
                    "lastName": "Galdikas"
