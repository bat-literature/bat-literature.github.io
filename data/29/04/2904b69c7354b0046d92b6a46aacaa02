            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Oliva S.",
                    "lastName": "Noroalintseheno Lalarivoniaina"
                },
                {
                    "creatorType": "author",
                    "firstName": "Faneva I.",
                    "lastName": "Rajemison"
