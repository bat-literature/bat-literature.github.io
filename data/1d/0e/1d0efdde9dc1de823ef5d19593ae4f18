            "version": 58469,
            "itemType": "journalArticle",
            "title": "Functional diversity and trait–environment relationships of stream fish assemblages in a large tropical catchment",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Allison A.",
                    "lastName": "Pease"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alfonso A.",
                    "lastName": "González‐Díaz"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rocío",
                    "lastName": "Rodiles‐Hernández"
