        "data": {
            "key": "KX5EAIFB",
            "version": 11579,
            "itemType": "journalArticle",
            "title": "Meta-analysis reveals glucocorticoid levels reflect variation in metabolic rate, not stress",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Blanca",
                    "lastName": "Jimeno"
                },
                {
                    "creatorType": "author",
                    "firstName": "Simon",
                    "lastName": "Verhulst"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Yuuki Y",
