                {
                    "creatorType": "author",
                    "firstName": "Xenia",
                    "lastName": "Mathgen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Aenne",
                    "lastName": "Douwes"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jaap",
                    "lastName": "Van Schaik"
                },
                {
                    "creatorType": "editor",
                    "firstName": "Marcus",
