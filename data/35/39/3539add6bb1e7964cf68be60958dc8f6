            "version": 55280,
            "itemType": "journalArticle",
            "title": "MPTP toxicity causes vocal, auditory, orientation and movement defects in the echolocation bat",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Wan-Jhen",
                    "lastName": "Wu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Chen-Wen",
                    "lastName": "Lu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sheue-Er",
                    "lastName": "Wang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ching-Lung",
