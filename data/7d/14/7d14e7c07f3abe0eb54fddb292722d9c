            "key": "SM5BDDPS",
            "version": 61277,
            "itemType": "journalArticle",
            "title": "Attitudes towards Bats in Swedish History",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Johan",
                    "lastName": "Eklöf"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jens",
                    "lastName": "Rydell"
                }
