            "version": 61268,
            "itemType": "journalArticle",
            "title": "Disparities in second-generation DNA metabarcoding results  exposed with accessible and repeatable workflows",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Timothy J.",
                    "lastName": "Divoll"
                },
                {
                    "creatorType": "author",
                    "firstName": "Veronica A.",
                    "lastName": "Brown"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jeff",
                    "lastName": "Kinne"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gary F.",
                    "lastName": "McCracken"
