                {
                    "creatorType": "author",
                    "firstName": "S.",
                    "lastName": "Loul"
                },
                {
                    "creatorType": "author",
                    "firstName": "E.",
                    "lastName": "Mpoudi-Ngole"
                },
                {
                    "creatorType": "author",
                    "firstName": "A.",
                    "lastName": "Vandamme"
                },
                {
                    "creatorType": "author",
                    "firstName": "E.",
                    "lastName": "Delaporte"
                },
