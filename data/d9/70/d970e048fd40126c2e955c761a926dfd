            "itemType": "journalArticle",
            "title": "Vocal performance reflects individual quality in male Great Himalayan leaf‐nosed bats (Hipposideros armiger)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Congnan",
                    "lastName": "Sun"
                },
                {
                    "creatorType": "author",
                    "firstName": "Chunmian",
                    "lastName": "Zhang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jeffrey R.",
                    "lastName": "Lucas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hao",
                    "lastName": "Gu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jiang",
