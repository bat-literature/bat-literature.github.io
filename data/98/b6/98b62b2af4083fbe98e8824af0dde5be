            "version": 17550,
            "itemType": "journalArticle",
            "title": "Review: Allee effects in social species",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Elena",
                    "lastName": "Angulo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gloria M.",
                    "lastName": "Luque"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stephen D.",
                    "lastName": "Gregory"
                },
                {
                    "creatorType": "author",
                    "firstName": "John W.",
                    "lastName": "Wenzel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carmen",
                    "lastName": "Bessa‐Gomes"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ludek",
                    "lastName": "Berec"
                },
                {
                    "creatorType": "author",
                    "firstName": "Franck",
                    "lastName": "Courchamp"
                },
