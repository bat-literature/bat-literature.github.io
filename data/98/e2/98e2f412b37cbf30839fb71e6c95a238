            "version": 54969,
            "itemType": "journalArticle",
            "title": "Moth tails divert bat attack: Evolution of acoustic deflection",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jesse R.",
                    "lastName": "Barber"
                },
                {
                    "creatorType": "author",
                    "firstName": "Brian C.",
                    "lastName": "Leavell"
                },
