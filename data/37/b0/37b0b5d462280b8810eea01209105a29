            "version": 61334,
            "itemType": "journalArticle",
            "title": "Female dietary bias towards large migratory moths in the European free-tailed bat (Tadarida teniotis)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Vanessa A.",
                    "lastName": "Mata"
                },
                {
                    "creatorType": "author",
                    "firstName": "Francisco",
                    "lastName": "Amorim"
                },
                {
                    "creatorType": "author",
                    "firstName": "Martin F. V.",
                    "lastName": "Corley"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gary F.",
                    "lastName": "McCracken"
