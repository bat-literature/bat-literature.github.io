            "version": 61355,
            "itemType": "journalArticle",
            "title": "Lord of the Diptera (and Moths and a Spider): Molecular Diet Analyses and Foraging Ecology of Indiana Bats in Illinois",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Devon R.",
                    "lastName": "O'Rourke"
                },
                {
                    "creatorType": "author",
                    "firstName": "Matthew T.",
                    "lastName": "Mangan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Karen E.",
                    "lastName": "Mangan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nicholas A.",
                    "lastName": "Bokulich"
                },
                {
                    "creatorType": "author",
                    "firstName": "Matthew D.",
