            "version": 53970,
            "itemType": "journalArticle",
            "title": "Phylogeography and Taxonomic Status of the Greater Mouse-Tailed Bat Rhinopoma microphyllum (Chiroptera: Rhinopomatidae) in Iran",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Vahid",
                    "lastName": "Akmali"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ali",
                    "lastName": "Farazmand"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jamshid",
