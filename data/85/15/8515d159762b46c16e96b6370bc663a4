            "version": 53950,
            "itemType": "journalArticle",
            "title": "Mitochondrial DNA confirms low genetic variation of the greater mouse-eared bats, Myotis myotis, in Central Europe",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Josef",
                    "lastName": "Bryja"
                },
                {
                    "creatorType": "author",
                    "firstName": "Marcel",
                    "lastName": "Uhrin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Peter",
                    "lastName": "Kaňuch"
                },
                {
                    "creatorType": "author",
                    "firstName": "Petra",
                    "lastName": "Bémová"
                },
                {
                    "creatorType": "author",
                    "firstName": "Natália",
