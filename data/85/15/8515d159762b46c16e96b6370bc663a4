        "data": {
            "key": "8364HTHS",
            "version": 53981,
            "itemType": "journalArticle",
            "title": "Postnatal growth and age estimation in big-footed myotis, Myotis macrodactylus",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ying",
                    "lastName": "Liu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Long-Ru",
                    "lastName": "Jin"
