            "key": "ZNVGP3C2",
            "version": 55020,
            "itemType": "journalArticle",
            "title": "Bats actively track and prey on grape pest populations",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Unai",
                    "lastName": "Baroja"
                },
                {
                    "creatorType": "author",
                    "firstName": "Inazio",
                    "lastName": "Garin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Nerea",
                    "lastName": "Vallejo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joxerra",
                    "lastName": "Aihartza"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hugo",
                    "lastName": "Rebelo"
