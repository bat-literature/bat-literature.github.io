            "version": 11600,
            "itemType": "journalArticle",
            "title": "A Generic Quantitative Risk Assessment Framework for the Entry of Bat-Borne Zoonotic Viruses into the European Union",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Robin R. L.",
                    "lastName": "Simons"
                },
                {
                    "creatorType": "author",
                    "firstName": "Verity",
                    "lastName": "Horigan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul",
                    "lastName": "Gale"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rowena D.",
                    "lastName": "Kosmider"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrew C.",
