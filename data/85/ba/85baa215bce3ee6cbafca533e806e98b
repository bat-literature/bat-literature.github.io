        "data": {
            "key": "QXI5ZT9D",
            "version": 11586,
            "itemType": "journalArticle",
            "title": "MERS coronavirus: diagnostics, epidemiology and transmission",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ian M.",
                    "lastName": "Mackay"
                },
                {
                    "creatorType": "author",
                    "firstName": "Katherine E.",
                    "lastName": "Arden"
