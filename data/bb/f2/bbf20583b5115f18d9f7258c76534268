            "version": 58293,
            "itemType": "journalArticle",
            "title": "Physiological stress and Hendra virus in flying-foxes (Pteropus spp.), Australia",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Lee",
                    "lastName": "McMichael"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel",
                    "lastName": "Edson"
                },
                {
                    "creatorType": "author",
                    "firstName": "Craig",
                    "lastName": "Smith"
                },
                {
                    "creatorType": "author",
                    "firstName": "David",
                    "lastName": "Mayer"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ina",
                    "lastName": "Smith"
                },
                {
                    "creatorType": "author",
                    "firstName": "Steven",
                    "lastName": "Kopp"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joanne",
