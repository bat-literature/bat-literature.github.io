            "key": "785GQCTX",
            "version": 58294,
            "itemType": "journalArticle",
            "title": "Using weather radar to monitor the number, timing and directions of flying-foxes emerging from their roosts",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jessica",
                    "lastName": "Meade"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rodney",
                    "lastName": "van der Ree"
                },
                {
                    "creatorType": "author",
                    "firstName": "Phillip M.",
                    "lastName": "Stepanian"
                },
                {
                    "creatorType": "author",
                    "firstName": "David A.",
                    "lastName": "Westcott"
