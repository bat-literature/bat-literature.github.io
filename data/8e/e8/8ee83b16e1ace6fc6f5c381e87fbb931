            "key": "DHNPYX8P",
            "version": 17630,
            "itemType": "journalArticle",
            "title": "Functional diversity and trait filtering of insectivorous bats on forest islands created by an Amazonian mega dam",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Guthieri Teixeira",
                    "lastName": "Colombo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Raffaello",
                    "lastName": "Di Ponzio"
                },
                {
                    "creatorType": "author",
                    "firstName": "Maíra",
                    "lastName": "Benchimol"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carlos A.",
                    "lastName": "Peres"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paulo Estefano D.",
                    "lastName": "Bobrowiec"
                }
