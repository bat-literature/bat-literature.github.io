            "version": 35250,
            "itemType": "journalArticle",
            "title": "Automated proximity sensing in small vertebrates: design of miniaturized sensor nodes and first field tests in bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Simon",
                    "lastName": "Ripperger"
                },
                {
                    "creatorType": "author",
                    "firstName": "Darija",
                    "lastName": "Josic"
                },
                {
                    "creatorType": "author",
                    "firstName": "Martin",
                    "lastName": "Hierold"
                },
