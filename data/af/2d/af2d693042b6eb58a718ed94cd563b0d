            "key": "KNXJWW36",
            "version": 37748,
            "itemType": "journalArticle",
            "title": "Oceanic records of North American bats and implications for offshore wind energy development in the United States",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Donald I.",
                    "lastName": "Solick"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christian M.",
                    "lastName": "Newman"
                }
