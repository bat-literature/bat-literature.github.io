                {
                    "creatorType": "author",
                    "firstName": "Benjamin",
                    "lastName": "Allegrini"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anna",
                    "lastName": "Bastian"
                },
                {
                    "creatorType": "author",
                    "firstName": "Gregory L.",
                    "lastName": "Mutumi"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tinyiko G.",
                    "lastName": "Maluleke"
