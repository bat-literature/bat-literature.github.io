            "version": 11572,
            "itemType": "journalArticle",
            "title": "Genome-wide screening of SARS-CoV-2 infection-related genes based on the blood leukocytes sequencing data set of patients with COVID-19",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Xin",
                    "lastName": "Gao"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yuan",
                    "lastName": "Liu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shaohui",
                    "lastName": "Zou"
                },
                {
                    "creatorType": "author",
                    "firstName": "Pengqin",
