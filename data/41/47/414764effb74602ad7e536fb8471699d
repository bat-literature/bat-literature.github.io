            "itemType": "journalArticle",
            "title": "Occupancy and Detectability of Northern Long‐eared Bats in the Lake States Region",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Brenna A.",
                    "lastName": "Hyzy"
                },
                {
                    "creatorType": "author",
                    "firstName": "Robin E.",
                    "lastName": "Russell"
                },
