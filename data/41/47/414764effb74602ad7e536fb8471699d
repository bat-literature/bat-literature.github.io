            "key": "WCSK2RL4",
            "version": 56865,
            "itemType": "journalArticle",
            "title": "Hepeviridae: An expanding family of vertebrate viruses",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Reimar",
                    "lastName": "Johne"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul",
                    "lastName": "Dremsek"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jochen",
                    "lastName": "Reetz"
