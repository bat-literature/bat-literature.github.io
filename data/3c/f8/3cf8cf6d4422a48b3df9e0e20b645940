            "version": 34973,
            "itemType": "journalArticle",
            "title": "Sex-specific roost movements and population dynamics of the vulnerable long-fingered bat, Myotis capaccinii",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Eleni",
                    "lastName": "Papadatou"
                },
                {
                    "creatorType": "author",
                    "firstName": "Roger K.",
                    "lastName": "Butlin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Roger",
