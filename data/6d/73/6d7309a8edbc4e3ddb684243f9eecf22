            "key": "IV27UAKB",
            "version": 17297,
            "itemType": "journalArticle",
            "title": "Ecology and evolutionary biology of fishing bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ostaizka",
                    "lastName": "Aizpurua"
                },
                {
                    "creatorType": "author",
                    "firstName": "Antton",
                    "lastName": "Alberdi"
                }
