        "data": {
            "key": "3FN72L7K",
            "version": 58404,
            "itemType": "journalArticle",
            "title": "Traits, phylogeny and host cell receptors predict Ebolavirus host status among African mammals",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Mekala",
                    "lastName": "Sundaram"
                },
                {
                    "creatorType": "author",
                    "firstName": "John Paul",
                    "lastName": "Schmidt"
                },
                {
                    "creatorType": "author",
                    "firstName": "Barbara A.",
                    "lastName": "Han"
                },
                {
                    "creatorType": "author",
                    "firstName": "John M.",
