            "version": 38526,
            "itemType": "journalArticle",
            "title": "Infection status and molecular detection of pathogens carried by ectoparasites of Miniopterus fuliginosus bats in Yunnan, China",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jinting",
                    "lastName": "Yang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yujian",
                    "lastName": "Wang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Huijuan",
                    "lastName": "Yang"
                },
                {
                    "creatorType": "author",
                    "firstName": "Xianzheng",
                    "lastName": "Zhang"
                },
