            "version": 56208,
            "itemType": "journalArticle",
            "title": "Phylogeny and biogeography of western Indian Ocean Rousettus (Chiroptera: Pteropodidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Steven M.",
                    "lastName": "Goodman"
                },
                {
                    "creatorType": "author",
                    "firstName": "Lauren M.",
                    "lastName": "Chan"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michael D.",
