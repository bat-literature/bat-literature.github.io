            "version": 58539,
            "itemType": "journalArticle",
            "title": "Current and potential distribution of Myotis simus (Chiroptera, Vespertilionidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ricardo",
                    "lastName": "Moratelli"
                },
                {
                    "creatorType": "author",
                    "firstName": "Cecilia S.",
                    "lastName": "De Andreazzi"
                },
                {
                    "creatorType": "author",
                    "firstName": "João A.",
