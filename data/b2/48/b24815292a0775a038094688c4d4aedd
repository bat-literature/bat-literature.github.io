            "version": 58499,
            "itemType": "journalArticle",
            "title": "Haematological, serum biochemical and electrophoretic data on healthy captive Egyptian fruit bats (Rousettus aegyptiacus)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Pierangelo",
                    "lastName": "Moretti"
                },
                {
                    "creatorType": "author",
                    "firstName": "Giuliano",
                    "lastName": "Ravasio"
                },
                {
                    "creatorType": "author",
                    "firstName": "William",
                    "lastName": "Magnone"
                },
