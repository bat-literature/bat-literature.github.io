            "version": 35274,
            "itemType": "journalArticle",
            "title": "Quantifying ecological and economic value of pest control services provided by bats in a vineyard landscape of central Chile",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Annia",
                    "lastName": "Rodríguez-San Pedro"
                },
                {
                    "creatorType": "author",
                    "firstName": "Juan Luis",
                    "lastName": "Allendes"
                },
                {
                    "creatorType": "author",
                    "firstName": "Clemente A.",
                    "lastName": "Beltrán"
                },
                {
                    "creatorType": "author",
                    "firstName": "Pascal N.",
                    "lastName": "Chaperon"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mónica M.",
                    "lastName": "Saldarriaga-Córdoba"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrea X.",
