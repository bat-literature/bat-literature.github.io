                {
                    "creatorType": "author",
                    "firstName": "James L. N.",
                    "lastName": "Wood"
                },
                {
                    "creatorType": "author",
                    "firstName": "Andrew A.",
                    "lastName": "Cunningham"
                },
                {
                    "creatorType": "author",
