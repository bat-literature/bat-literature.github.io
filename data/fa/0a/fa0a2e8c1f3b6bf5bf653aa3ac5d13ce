            "version": 56420,
            "itemType": "journalArticle",
            "title": "Evolution of scapula shape in several families of bats (Chiroptera, Mammalia)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Pablo J.",
                    "lastName": "Gaudioso"
                },
                {
                    "creatorType": "author",
                    "firstName": "Juan J.",
