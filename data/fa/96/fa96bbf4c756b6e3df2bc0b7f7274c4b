                },
                {
                    "creatorType": "author",
                    "firstName": "Luciano",
                    "lastName": "Bosso"
                },
                {
                    "creatorType": "author",
                    "firstName": "Sonia",
                    "lastName": "Smeraldo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Víctor",
                    "lastName": "Sánchez Cordero"
