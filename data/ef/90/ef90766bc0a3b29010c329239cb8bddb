            "key": "Z3GKN8TD",
            "version": 38459,
            "itemType": "journalArticle",
            "title": "A goodness‐of‐fit test for occupancy models with correlated within‐season revisits",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Wilson J.",
                    "lastName": "Wright"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kathryn M.",
                    "lastName": "Irvine"
