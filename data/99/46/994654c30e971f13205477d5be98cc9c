            "version": 61350,
            "itemType": "journalArticle",
            "title": "Environmental niche drives genetic and morphometric structure in a widespread bat",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ariadna",
                    "lastName": "Morales"
                },
                {
                    "creatorType": "author",
                    "firstName": "Fabricio",
                    "lastName": "Villalobos"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paúl M.",
