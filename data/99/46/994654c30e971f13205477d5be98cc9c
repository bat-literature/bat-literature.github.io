            "version": 61345,
            "itemType": "journalArticle",
            "title": "Rapid range expansion of the Brazilian free-tailed bat in the southeastern United States, 2008–2016",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Gary F",
                    "lastName": "McCracken"
                },
                {
                    "creatorType": "author",
                    "firstName": "Riley F",
                    "lastName": "Bernard"
                },
                {
                    "creatorType": "author",
                    "firstName": "Melquisidec",
                    "lastName": "Gamba-Rios"
                },
                {
                    "creatorType": "author",
                    "firstName": "Randy",
                    "lastName": "Wolfe"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jennifer J",
                    "lastName": "Krauel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Devin N",
                    "lastName": "Jones"
                },
                {
                    "creatorType": "author",
                    "firstName": "Amy L",
                    "lastName": "Russell"
