            "title": "Pollination Syndromes and Floral Specialization",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Charles B.",
                    "lastName": "Fenster"
                },
                {
                    "creatorType": "author",
                    "firstName": "W. Scott",
