            "volume": "39",
            "issue": "5",
            "pages": "560-566",
            "date": "08/2014",
            "series": "",
            "seriesTitle": "",
            "seriesText": "",
            "journalAbbreviation": "Austral Ecology",
            "language": "en",
            "DOI": "10.1111/aec.12116",
