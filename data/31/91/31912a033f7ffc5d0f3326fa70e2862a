                },
                {
                    "creatorType": "author",
                    "firstName": "Chutamas",
                    "lastName": "Satasook"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paul J. J.",
                    "lastName": "Bates"
                },
                {
                    "creatorType": "author",
                    "firstName": "Pipat",
