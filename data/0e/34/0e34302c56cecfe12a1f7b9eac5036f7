            "title": "Dilution effects in disease ecology",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Felicia",
                    "lastName": "Keesing"
                },
                {
                    "creatorType": "author",
                    "firstName": "Richard S.",
                    "lastName": "Ostfeld"
                }
