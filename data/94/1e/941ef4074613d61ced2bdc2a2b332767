            "itemType": "journalArticle",
            "title": "Availability of Food for Frugivorous Bats in Lowland Amazonia: The Influence of Flooding and of River Banks",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "João Tiago",
                    "lastName": "Marques"
                },
                {
                    "creatorType": "author",
                    "firstName": "Maria João Ramos",
                    "lastName": "Pereira"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jorge M.",
                    "lastName": "Palmeirim"
                }
