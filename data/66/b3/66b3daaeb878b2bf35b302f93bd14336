            "version": 56132,
            "itemType": "journalArticle",
            "title": "Eye shape and the nocturnal bottleneck of mammals",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Margaret I.",
                    "lastName": "Hall"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jason M.",
                    "lastName": "Kamilar"
