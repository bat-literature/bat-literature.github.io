            "version": 32483,
            "itemType": "journalArticle",
            "title": "The crust of a male: does size matter when females are fertile?",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Mariana",
                    "lastName": "Muñoz-Romo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Victoria",
                    "lastName": "Flores"
                },
                {
                    "creatorType": "author",
                    "firstName": "Paolo",
