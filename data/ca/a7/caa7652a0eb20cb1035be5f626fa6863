                },
                {
                    "creatorType": "author",
                    "firstName": "Carleton J.",
                    "lastName": "Phillips"
                },
                {
                    "creatorType": "author",
                    "firstName": "Dorothy E.",
                    "lastName": "Pumo"
                },
