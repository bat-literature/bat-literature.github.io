            "version": 17315,
            "itemType": "journalArticle",
            "title": "First findings and phenology of Hypsugo savii in the Děčín District, Czech Republic (Chiroptera: Vespertilionidae)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Tomáš",
                    "lastName": "Bartonička"
                },
                {
                    "creatorType": "author",
                    "firstName": "Pavel",
