            "key": "N7PHEI8B",
            "version": 17629,
            "itemType": "journalArticle",
            "title": "Mitogenomic relationships of placental mammals and molecular estimates of their divergences",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Ulfur",
                    "lastName": "Arnason"
                },
                {
                    "creatorType": "author",
                    "firstName": "Joseph A.",
                    "lastName": "Adegoke"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anette",
                    "lastName": "Gullberg"
