            "key": "AE5PWMGX",
            "version": 17654,
            "itemType": "journalArticle",
            "title": "Responses of bat-prey to prescribed burns and mechanical thinning",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Aaron P.",
                    "lastName": "Tormanen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jorista",
                    "lastName": "Garrie"
