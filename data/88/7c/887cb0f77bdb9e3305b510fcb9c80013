            "version": 55549,
            "itemType": "journalArticle",
            "title": "Exceptionally Uniform Bat Assemblages across Different Forest Habitats Are Dominated by Single Hyperabundant Generalist Species",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Mateusz",
                    "lastName": "Ciechanowski"
                },
                {
                    "creatorType": "author",
                    "firstName": "Zuzanna",
                    "lastName": "Wikar"
                },
                {
                    "creatorType": "author",
                    "firstName": "Katarzyna",
                    "lastName": "Borzym"
                },
                {
                    "creatorType": "author",
                    "firstName": "Emilia",
