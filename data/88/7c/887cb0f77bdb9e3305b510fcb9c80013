            "key": "PCIDNGN6",
            "version": 55532,
            "itemType": "journalArticle",
            "title": "Retinal ganglion cell topography and spatial resolving power in African megachiropterans: Influence of roosting microhabitat and foraging",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "João Paulo",
                    "lastName": "Coimbra"
                },
                {
                    "creatorType": "author",
                    "firstName": "John D.",
                    "lastName": "Pettigrew"
                },
                {
                    "creatorType": "author",
                    "firstName": "Consolate",
                    "lastName": "Kaswera‐Kyamakya"
                },
                {
                    "creatorType": "author",
                    "firstName": "Emmanuel",
                    "lastName": "Gilissen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Shaun P.",
                    "lastName": "Collin"
                },
