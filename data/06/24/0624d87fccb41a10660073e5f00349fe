                },
                {
                    "creatorType": "author",
                    "firstName": "Natalia J.",
                    "lastName": "Pérez-Amaya"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alex Mauricio",
                    "lastName": "Jiménez-Ortega"
                },
                {
                    "creatorType": "author",
                    "firstName": "Hugo",
