            "version": 18104,
            "itemType": "journalArticle",
            "title": "A brief history of fruits and frugivores",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Theodore H.",
                    "lastName": "Fleming"
                },
                {
                    "creatorType": "author",
                    "firstName": "W.",
                    "lastName": "John Kress"
                }
