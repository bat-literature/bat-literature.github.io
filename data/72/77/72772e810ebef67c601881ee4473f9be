                },
                {
                    "creatorType": "author",
                    "firstName": "Dylan",
                    "lastName": "Baloun"
                },
                {
                    "creatorType": "author",
                    "firstName": "Trent K.",
                    "lastName": "Bollinger"
                },
                {
                    "creatorType": "author",
                    "firstName": "Vikram",
                    "lastName": "Misra"
