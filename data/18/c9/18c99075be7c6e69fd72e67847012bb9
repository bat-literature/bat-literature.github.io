            "key": "IFPYAGV5",
            "version": 56132,
            "itemType": "journalArticle",
            "title": "Predicting habitat suitability for Townsend's big‐eared bats across California in relation to climate change",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Natalie M.",
                    "lastName": "Hamilton"
                },
                {
                    "creatorType": "author",
                    "firstName": "Michael L.",
                    "lastName": "Morrison"
                },
                {
                    "creatorType": "author",
                    "firstName": "Leila S.",
                    "lastName": "Harris"
                },
