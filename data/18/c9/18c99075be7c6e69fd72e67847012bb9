            "title": "Extracellular matrix composition of different spleen compartments of fruit bats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Desrayni",
                    "lastName": "Hanadhita"
                },
                {
                    "creatorType": "author",
                    "firstName": "Anisa",
                    "lastName": "Rahma"
                },
