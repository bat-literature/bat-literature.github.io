            "key": "YAFU9QL2",
            "version": 29358,
            "itemType": "journalArticle",
            "title": "Nest-site competition and killing by invasive parakeets cause the decline of a threatened bat population",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Dailos",
                    "lastName": "Hernández-Brito"
                },
                {
                    "creatorType": "author",
                    "firstName": "Martina",
                    "lastName": "Carrete"
                },
                {
                    "creatorType": "author",
                    "firstName": "Carlos",
                    "lastName": "Ibáñez"
