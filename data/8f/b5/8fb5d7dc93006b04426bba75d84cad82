            "version": 57453,
            "itemType": "journalArticle",
            "title": "RESPONSE OF NORTHERN BATS (MYOTIS SEPTENTRIONALIS) TO PRESCRIBED FIRES IN EASTERN KENTUCKY FORESTS",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Michael J.",
                    "lastName": "Lacki"
                },
                {
                    "creatorType": "author",
                    "firstName": "Daniel R.",
                    "lastName": "Cox"
                },
                {
                    "creatorType": "author",
                    "firstName": "Luke E.",
