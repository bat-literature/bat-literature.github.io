            "version": 17740,
            "itemType": "journalArticle",
            "title": "Defensive fruit metabolites obstruct seed dispersal by altering bat behavior and physiology at multiple temporal scales",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Justin W.",
                    "lastName": "Baldwin"
                },
                {
                    "creatorType": "author",
                    "firstName": "Dina K. N.",
                    "lastName": "Dechmann"
                },
                {
                    "creatorType": "author",
                    "firstName": "Wibke",
                    "lastName": "Thies"
