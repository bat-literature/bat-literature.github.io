            "itemType": "journalArticle",
            "title": "The Antioxidant Status of Three Neotropical Bat Species with Different Feeding Habits",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Renata M.",
                    "lastName": "Pereira Freitas"
                },
                {
                    "creatorType": "author",
                    "firstName": "Jerusa M.",
                    "lastName": "Oliveira"
                },
                {
                    "creatorType": "author",
                    "firstName": "David L.",
                    "lastName": "Justinico Castro"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mariaurea Matias",
                    "lastName": "Sarandy"
                },
