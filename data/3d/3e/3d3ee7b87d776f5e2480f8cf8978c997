            "version": 35018,
            "itemType": "journalArticle",
            "title": "Oxidative Stress and Antioxidant Defense in the Heart, Liver, and Kidney of Bat Species with Different Feeding Habits",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Francielly Dias",
                    "lastName": "Pereira"
                },
                {
                    "creatorType": "author",
                    "firstName": "Diego Antonio",
                    "lastName": "Mena Canata"
                },
                {
                    "creatorType": "author",
                    "firstName": "Tiago Boeira",
                    "lastName": "Salomon"
                },
                {
                    "creatorType": "author",
                    "firstName": "Fernanda Schäfer",
                    "lastName": "Hackenhaar"
                },
                {
                    "creatorType": "author",
                    "firstName": "María João Ramos",
                    "lastName": "Pereira"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mara Silveira",
