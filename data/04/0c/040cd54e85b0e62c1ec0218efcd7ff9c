            "version": 57962,
            "itemType": "journalArticle",
            "title": "Analysis of Genomic DNA Methylation in Six Tissues of the Greater Horseshoe Bat (Rhinolophus ferrumequinum)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Sen",
                    "lastName": "Liu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Yue",
                    "lastName": "Zhu"
                },
                {
                    "creatorType": "author",
                    "firstName": "Haixia",
                    "lastName": "Leng"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ying",
