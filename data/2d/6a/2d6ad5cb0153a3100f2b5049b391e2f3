            "key": "QZK3LAVD",
            "version": 17015,
            "itemType": "journalArticle",
            "title": "Insectivorous bats are less active near freeways",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Manisha",
                    "lastName": "Bhardwaj"
                },
                {
                    "creatorType": "author",
                    "firstName": "Kylie",
                    "lastName": "Soanes"
                },
                {
                    "creatorType": "author",
                    "firstName": "José J.",
                    "lastName": "Lahoz-Monfort"
                },
                {
                    "creatorType": "author",
                    "firstName": "Linda F.",
                    "lastName": "Lumsden"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rodney",
                    "lastName": "Van Der Ree"
                },
