            "version": 58612,
            "itemType": "journalArticle",
            "title": "Microbat paraphyly and the convergent evolution of a key innovation in Old World rhinolophoid microbats",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Emma C.",
                    "lastName": "Teeling"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ole",
                    "lastName": "Madsen"
                },
                {
                    "creatorType": "author",
                    "firstName": "Ronald A.",
                    "lastName": "Van Den Bussche"
