        "data": {
            "key": "SADLCSEG",
            "version": 58479,
            "itemType": "journalArticle",
            "title": "Mitochondrial DNA relationships among North Palaearctic Eptesicus (Vespertilionidae, Chiroptera) and past hybridization between Common Serotine and Northern Bat",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Artyushin I.",
                    "lastName": "V."
                },
                {
                    "creatorType": "author",
                    "firstName": "Bannikova A.",
                    "lastName": "A."
                },
                {
                    "creatorType": "author",
                    "firstName": "Lebedev V.",
