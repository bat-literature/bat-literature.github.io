            "version": 47845,
            "itemType": "journalArticle",
            "title": "Migratory stopover in the long-distance migrant silverhaired bat, Lasionycteris noctivagans",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Liam P.",
                    "lastName": "McGuire"
                },
                {
                    "creatorType": "author",
                    "firstName": "Christopher G.",
                    "lastName": "Guglielmo"
                },
                {
                    "creatorType": "author",
                    "firstName": "Stuart A.",
                    "lastName": "Mackenzie"
