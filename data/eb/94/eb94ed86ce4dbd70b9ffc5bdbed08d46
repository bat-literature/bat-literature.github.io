            "version": 58582,
            "itemType": "journalArticle",
            "title": "A new species of Australian freetail bat Mormopterus eleryi sp. nov. (Chiroptera: Molossidae) and a taxonomic reappraisal of M. norfolkensis (Gray)",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Terry",
                    "lastName": "Reardon"
                },
                {
                    "creatorType": "author",
                    "firstName": "Mark",
                    "lastName": "Adams"
                },
