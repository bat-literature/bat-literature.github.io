            "key": "IUYLNCAN",
            "version": 28983,
            "itemType": "journalArticle",
            "title": "Species-specific barrier effects of a motorway on the habitat use of two threatened forest-living bat species",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Gerald",
                    "lastName": "Kerth"
                },
                {
                    "creatorType": "author",
                    "firstName": "Markus",
                    "lastName": "Melber"
                }
