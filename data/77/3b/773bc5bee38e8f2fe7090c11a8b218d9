        "data": {
            "key": "8DR4DG2J",
            "version": 11591,
            "itemType": "journalArticle",
            "title": "The preregistration revolution.",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Brian A",
                    "lastName": "Nosek"
                },
                {
                    "creatorType": "author",
                    "firstName": "Charles R",
                    "lastName": "Ebersole"
                },
                {
                    "creatorType": "author",
                    "firstName": "Alexander C",
