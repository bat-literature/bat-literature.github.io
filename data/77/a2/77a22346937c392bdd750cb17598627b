            "itemType": "journalArticle",
            "title": "Wingbeat frequency of barn swallows and house martins",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "F",
                    "lastName": "Liechti"
                },
                {
                    "creatorType": "author",
                    "firstName": "L",
                    "lastName": "Bruderer"
                }
