            "key": "88ANHAGK",
            "version": 17795,
            "itemType": "journalArticle",
            "title": "A spatial approach to combatting wildlife crime",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "S. C.",
                    "lastName": "Faulkner"
                },
                {
                    "creatorType": "author",
                    "firstName": "M. C. A.",
                    "lastName": "Stevens"
                },
                {
                    "creatorType": "author",
                    "firstName": "S. S.",
