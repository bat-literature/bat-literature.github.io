            "key": "ASRE5NFU",
            "version": 17304,
            "itemType": "journalArticle",
            "title": "Changes in activity along the year in a community of insectivorous bats inhabiting a montane ecosystem of central Mexico",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Jorge",
                    "lastName": "Ayala-Berdon"
                },
                {
                    "creatorType": "author",
                    "firstName": "Rommy",
                    "lastName": "Vázquez-Fuerte"
                },
                {
                    "creatorType": "author",
                    "firstName": "Antonio",
                    "lastName": "Guillén-Servent"
                },
                {
                    "creatorType": "author",
                    "firstName": "Issachar L.",
                    "lastName": "López-Cuamatzi"
