            "key": "LEBKHR45",
            "version": 17637,
            "itemType": "journalArticle",
            "title": "Flexible echolocation behavior of trawling bats during approach of continuous or transient prey cues",
            "creators": [
                {
                    "creatorType": "author",
                    "firstName": "Kirstin",
                    "lastName": "Übernickel"
                },
                {
                    "creatorType": "author",
                    "firstName": "Marco",
                    "lastName": "Tschapka"
                },
                {
                    "creatorType": "author",
                    "firstName": "Elisabeth K. V.",
                    "lastName": "Kalko"
                }
